{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "PDF Compressor",
	"short_name": "pdf-compress-wasm",
	"version": "0.21",
	"icons": { "128": "logo.png" },
	"description": "Compress PDF using GhostScript in WebAssembly.",
	"manifest_version": 2,
	"background": {
		"scripts": [
		"background.js"
		]
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"<all_urls>",
		"webRequest",
		"webRequestBlocking"
	],
	"web_accessible_resources": ["viewer.html", "viewer.js"]
}
